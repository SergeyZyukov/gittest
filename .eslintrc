{
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "extends": ["airbnb/base"],
    "rules": {
        "semi":"error",
        "no-console": "warn",
        "no-plusplus": "off",
        "eol-last": "off",
        "max-len": ["error", 160, {"ignoreUrls": true}],
        "no-trailing-spaces":"off",
        "no-param-reassign":"off"
    }
}